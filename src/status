			if (file_exists(MAIN_HOME . 'bin/nginx/conf/codes/c.conf')) {
				StreamingUtilities::$db->query("DELETE FROM `access_codes` WHERE `code` = 'c';");
				unlink(MAIN_HOME . 'bin/nginx/conf/codes/c.conf');
				$rReload = true;
