#!/home/xc_vm/bin/php/bin/php
<?php

if (posix_getpwuid(posix_geteuid())['name'] == 'root') {
	set_time_limit(0);

	if ($argc) {
		$rReload = false;
		$rFirstRun = false;

		if (isset($argv[1])) {
			$rFirstRun = true;
		}

		require str_replace('\\', '/', dirname($argv[0])) . '/www/stream/init.php';
		ini_set('display_startup_errors', 1);
		ini_set('display_errors', 1);
		error_reporting(32767);

		if (!$rFirstRun) {
			echo "\n" . 'Status' . "\n" . '------------------------------' . "\n";

			if (isrunning()) {
				echo 'XC_VM is running.' . "\n\n";
			} else {
				echo 'XC_VM is not running.' . "\n\n";
			}
		} else {
			echo "\n";
		}

		echo 'Database' . "\n" . '------------------------------' . "\n";
		StreamingUtilities::connectDatabase();

		if (!StreamingUtilities::$db->connected) {
			echo "Couldn't connect to database. Please add them to config.ini." . "\n\n";

			exit();
		}

		echo 'Connected successfully.' . "\n\n";
		$rServers = abc6134f1458b898();

		if ($rServers[SERVER_ID]['is_main']) {
			StreamingUtilities::$db->query("SHOW INDEXES IN `servers` WHERE `key_name` = 'private_ip';");

			if (0 >= StreamingUtilities::$db->num_rows()) {
			} else {
				StreamingUtilities::$db->query('ALTER TABLE `servers` DROP INDEX `private_ip`;');
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `settings` LIKE 'hide_failures';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query("ALTER TABLE `settings` ADD COLUMN `hide_failures` TINYINT(4) NULL DEFAULT '0';");
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `settings` LIKE 'legacy_panel_api';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query("ALTER TABLE `settings` ADD COLUMN `legacy_panel_api` TINYINT(4) NULL DEFAULT '0';");
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `lines` LIKE 'allowed_outputs';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query('ALTER TABLE `lines` ADD COLUMN `allowed_outputs` MEDIUMTEXT NULL DEFAULT NULL AFTER `bouquet`;');
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `settings` LIKE 'check_connections';");

			if (0 >= StreamingUtilities::$db->num_rows()) {
			} else {
				StreamingUtilities::$db->query('ALTER TABLE `settings` DROP COLUMN `check_connections`;');
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `users` LIKE 'api_key';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query("ALTER TABLE `users` ADD COLUMN `api_key` VARCHAR(64) NULL DEFAULT '';");
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `users` LIKE 'lang';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query("ALTER TABLE `users` ADD COLUMN `lang` varchar(50) COLLATE utf8_unicode_ci DEFAULT 'en';");
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `settings` LIKE 'api_probe';");

			if (StreamingUtilities::$db->num_rows() == 0) {
				StreamingUtilities::$db->query("ALTER TABLE `settings` ADD COLUMN `api_probe` TINYINT(4) NULL DEFAULT '1';");
			}

			StreamingUtilities::$db->query("SHOW COLUMNS FROM `servers` LIKE 'last_status';");

			if (StreamingUtilities::$db->num_rows() == 0) {
