							StreamingUtilities::$db->query('SELECT * FROM `lines_output` LIMIT ' . $rStep . ', 1000;');
							$rResults = StreamingUtilities::$db->get_rows();

							foreach ($rResults as $rResult) {
								$rOutput[$rResult['user_id']][] = $rResult['access_output_id'];
